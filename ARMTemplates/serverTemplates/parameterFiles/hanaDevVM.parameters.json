{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "sapSystemId": {
      "value": "[SID]"
    },
    "adminUsername": {
      "value": "demoadmin"
    },
    "authenticationType": {
      "value": "password"
    },
    "adminPasswordOrKey": {
      "reference": {
        "keyVault": {
          "id": "[KeyVaultID]"
        },
        "secretName": "mypassword"
      }
    },
    "location": {
      "value": "[LOCATION]"
    },

    "virtualMachineName": {
      "value": "[SERVERNAME]"
    },
    "virtualMachineSize": {
      "value": "[MACHINESIZE]"
    },
    "privateIP": {
      "value": []
    },
    "HasPublicIP": {
      "value": "[HASPUBLICIP]"
    },
    "availabilitySetName": {
      "value": "[SID]-db-avset"
    },
    "proximityPlacementGroupName": {
      "value": "[SID]-ppg"
    },
    "imageReference": {
      "value": {
        "publisher": "[PUBLISHER]",
        "offer": "[OFFER]",
        "sku": "[SKU]",
        "version": "[VERSION]"
      }
    },
    "imageID": {
      "value": "[ImageID]"
    },
    "virtualMachineCount": {
      "value": "[VMCount]"
    },
    "applicationSecurityGroupName": {
      "value": "[DBASG]"
    },
    "virtualNetworkResourceGroupName": {
      "value": "[VNetRG]"
    },
    "virtualNetworkName": {
      "value": "[VNetName]"
    },
    "subnetName": {
      "value": "[DBSubnetName]"
    },
    
    "virtualMachineTagValues": {
      "value": { "System": "SAP", "$perfOptimizationLevel": "1" }
    },
    "Zone": {
        "value": []
    }
  }
}
